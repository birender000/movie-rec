{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-movie-recommend"
		},
		"ls_movie_data_store_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_movie_data_store'"
		},
		"ls_movie_notebook_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_movie_notebook'"
		},
		"ls_movie_notebook_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0216-162225-kie8e6fi"
		},
		"trigger-mr_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/9a1be070-51c7-4856-9b97-326a714b21a9/resourceGroups/movie-recommendation-rg/providers/Microsoft.Storage/storageAccounts/sa2mrlandingzone"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-movie-recommend_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_movie_data_store_connectionString": {
						"value": "[parameters('ls_movie_data_store_connectionString')]"
					},
					"ls_movie_notebook_accessToken": {
						"value": "[parameters('ls_movie_notebook_accessToken')]"
					},
					"ls_movie_notebook_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_movie_notebook_properties_typeProperties_existingClusterId')]"
					},
					"trigger-mr_properties_typeProperties_scope": {
						"value": "[parameters('trigger-mr_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}